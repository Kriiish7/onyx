{
  "name": "@onyx/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for the Onyx AI infrastructure engine â€” semantic search, knowledge graphs, and temporal versioning.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "deno": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "onyx",
    "sdk",
    "knowledge-graph",
    "vector-search",
    "ai",
    "semantic-search",
    "deno",
    "bun"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/onyx"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "msw": "^2.0.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18",
    "bun": ">=1.0",
    "deno": ">=1.38"
  }
}
